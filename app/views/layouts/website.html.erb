<!-- Login modal -->   
<div class="modal fade" id="login_2" tabindex="-1" role="dialog" aria-labelledby="myLogin" aria-hidden="true" style="z-index: 999 !important;">
		<div class="modal-dialog">
			<div class="modal-content modal-popup">
				<a href="#" class="close-link"><i class="icon_close_alt2"></i></a>
                <%= form_tag(website_signin_path , method: :post , class: 'popup-form') do %>
                	<div class="login_icon"><i class="icon_lock_alt"></i></div>
                    <%= text_field_tag :email, '' , class: 'form-control form-white' , placeholder: 'johndoe@example.com' %>
					<%= password_field_tag :password , '' , class: 'form-control form-white' , placeholder: 'Password' %>
					<div class="row">
                        <div class="col-md-6 text-left">
    						<a onclick="forget_pass();" style="cursor: pointer;">Forgot Password?</a>
    					</div>
                        <div class="col-md-6 text-left">
                            <a onclick="register();" style="cursor: pointer;">Register</a>
                        </div>
                    </div>
					<!-- <button type="submit" class="btn btn-submit">Submit</button> -->
                    <%= submit_tag "Submit", class: "btn btn-submit" %>
				<% end %>
			</div>
		</div>
	</div><!-- End modal -->   

    <!-- Login modal -->   
<div class="modal fade" id="forget" tabindex="-1" role="dialog" aria-labelledby="myLogin" aria-hidden="true" style="z-index: 999 !important;">
    <div class="modal-dialog">
        <div class="modal-content modal-popup">
            <a href="#" class="close-link"><i class="icon_close_alt2"></i></a>
            <%= form_tag(forget_password_website_path , method: :post , class: 'popup-form') do %>
                <div class="login_icon"><i class="icon_lock_alt"></i></div>
                <%= text_field_tag :email, '' , class: 'form-control form-white' , placeholder: 'johndoe@example.com' %>
                <div class="text-left">
                    <a onclick="signin();" style="cursor: pointer;">SignIn</a>
                </div>
                <!-- <button type="submit" class="btn btn-submit">Submit</button> -->
                <%= submit_tag "Forget Password", class: "btn btn-submit" %>
            <% end %>
        </div>
    </div>
</div><!-- End modal -->  
    
<!-- Register modal -->   
<div class="modal fade" id="register" tabindex="-1" role="dialog" aria-labelledby="myRegister" aria-hidden="true" style="z-index: 999 !important;">
	<div class="modal-dialog">
		<div class="modal-content modal-popup">
			<a href="#" class="close-link"><i class="icon_close_alt2"></i></a>
			<%= form_tag(website_signup_path , method: :post , class: 'popup-form') do %>
            	<div class="login_icon"><i class="icon_lock_alt"></i></div>
				<%= text_field_tag :name, '' , class: 'form-control form-white' , placeholder: 'John Doe' %>
                <%= text_field_tag :username, '' , class: 'form-control form-white' , placeholder: 'johndoe' %>
				<%= text_field_tag :email, '' , class: 'form-control form-white' , placeholder: 'johndoe@example.com' %>
                <%= password_field_tag :password , '' , class: 'form-control form-white' , placeholder: 'Password' %>
                <%= password_field_tag :c_password , '' , class: 'form-control form-white' , placeholder: 'Confirm Password' %>
                <!-- <div id="pass-info" class="clearfix"></div>
				<div class="checkbox-holder text-left">
					<div class="checkbox">
						<input type="checkbox" value="accept_2" id="check_2" name="check_2" />
						<label for="check_2"><span>I Agree to the <strong>Terms &amp; Conditions</strong></span></label>
					</div>
				</div> -->
				<!-- <button type="submit" class="btn btn-submit">Register</button> -->
                <%= submit_tag "Register", class: "btn btn-submit" , style: 'margin: 0px !important;'%>
			<% end %>
		</div>
	</div>
</div><!-- End Register modal -->
    
